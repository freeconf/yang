        type string {
            pattern '[a-z]*';
            pattern '^(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|'  +
              '25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4]'  +
              '[0-9]|25[0-5])$';
        }
        default "foo";
    }

    leaf l3 {
        type decimal64 {
            fraction-digits 10;
        }
    }

    leaf l4 {
        type string {
            pattern 'abc' {
                modifier invert-match;
            }
        }
    }
